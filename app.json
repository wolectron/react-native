{
  "expo": {
    "name": "FlexStream",
    "slug": "flexstream",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "splash": {
      "image": "./app/assets/flexstream2.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.flexstream.flexstreamapp",
      "buildNumber": "1.0.0"
    },
    "android": {
      "icon": "./app/assets/icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.flexstream.flexstreamapp",
      "versionCode": 3,
      "permissions": ["WRITE_EXTERNAL_STORAGE", "READ_EXTERNAL_STORAGE", "READ_INTERNAL_STORAGE", "NOTIFICATIONS"]
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    }
  }
}
